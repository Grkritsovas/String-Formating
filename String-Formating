def print_formatted(number):
    # your code goes here
    bina=bin(number).replace("0b","")
    n=str(bina)
    l=len(n)
    s=" "*l
    fs=" "*(l-1)
    for i in range(1,number+1):
        
        octal=str(oct(i).replace("0o",""))
        hexa=str(hex(i).replace("0x",""))
        bina=str(bin(i).replace("0b",""))
        print(fs+str(i)+s+octal+s+hexa+bina.rjust(l+1))
    
        

if __name__ == '__main__':
    n = int(input())
    print_formatted(n)
